<?xml version="1.0" encoding="UTF-8"?>
<openerp>
<data>

    <record id="view_partner_form" model="ir.ui.view">
        <field name="name">view.partner.form.view</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">
            <field name="title" position="after">
                <field name="external_laboratory"/>
            </field>
        </field>
    </record>

    <record id="view_res_partner_filter" model="ir.ui.view">
        <field name="name">view.partner.filter.view</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_res_partner_filter"/>
        <field name="arch" type="xml">
            <filter string="Suppliers" position="after">
                <filter string="External Laboratory" domain="[('external_laboratory', '=', True)]"/>
            </filter>
        </field>
    </record>

</data>
</openerp>
